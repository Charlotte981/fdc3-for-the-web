{
  "name": "@kite9/web-fdc3",
  "private": true,
  "version": "0.0.30",
  "workspaces": [
    "packages/fdc3-common",
    "packages/da-server",
    "packages/da-proxy",
    "packages/client",
    "packages/addon",
    "packages/fdc3-workbench",
    "packages/demo"
  ],
  "scripts": {
    "clean": "npm run clean --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces",
    "dev": "concurrently \"npm run dev -w addon\" \"npm run dev -w fdc3-workbench\" \"npm run build -w @kite9/da-server && npm run dev -w @kite9/demo\""
  },
  "packageManager": "npm@10.2.4",
  "devDependencies": {
    "concurrently": "8.2.2",
    "eslint": "7.17.0",
    "prettier": "2.2.1",
    "rimraf": "^5.0.7",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
